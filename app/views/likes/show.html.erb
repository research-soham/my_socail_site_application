<div class="container">
  <div class="container">
    <div>
      <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6", style=" padding-bottom:1%; padding-top:2%;"><h4>Likes</h4></div>
        
      </div>
      <% @accounts.each do |account| %>
        <div class="row">
          <div class="col-sm-3"></div>
          <% profile = account.profile %>
          <div class="col-sm-5", style="background-color:beige"><p><%= image_tag account.profile.profile_image, class: "img-thumbnail rounded-circle", style:"width:50px; height:50px" %><%= link_to profile.user_name, profile_path(profile.id) %></p></div>
          <div class="col-sm-1", style="background-color:beige; padding-top:1%;"><small style="color: blue">Liked</small></div>
          <div class="col-sm-3"></div>
        </div>
      <% end %>
    </div>
  </div>
</div>